<!-- Create Team -->
<section>
  <p>{{fail_count}} fails are required to fail this mission.</p>
  <form method="get" action="" id="create_team_form">
    <div class="row uniform">
      {{#players}}
      <div class="12u">
        <input type="checkbox" id="player_{{id}}" name="player_{{id}}" class="team_player"/>
        <label for="player_{{id}}">{{name}}</label>
      </div>
      {{/players}}
      <div class="12u">
        <input type="submit" class="button special fit disabled" value="Pick {{count}} more players" disabled="disabled"/>
      </div>
    </div>
  </form>
</section>
